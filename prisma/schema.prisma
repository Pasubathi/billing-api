// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_WITH_SCHEMA")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

model account__c {
    id Int @id @default(autoincrement())
    name__c String?
    phone__c String?
    email__c String? @unique
    password__c String?
    gstin_cc String?
    compant_name__c String?
    address__c String?
    pincode__c String?
    gst_composite__c String?
    business_nature__c String?
    entity_type__c String?
    cin__c String?
    pan__c String?
    website__c String?
    license_title__c String?
    license_number__c String?
    creaedDate DateTime? @default(now())
}

model customer__c {
    id Int @id @default(autoincrement())
    name__c String?
    phone__c String?
    email__c String? @unique
    gstin__c String?
    address__c String?
    entity_type__c String?
    license_title__c String?
    license_number__c String?
    account_id Int?
    created_date__c DateTime? @default(now())
}

model profile_c {
    id Int @id @default(autoincrement())
    phone__c String?
    email__c String? @unique
    gstin_c String?
    compant_name__c String?
    address__c String?
    pincode__c String?
    gst_composite__c String?
    business_nature__c String?
    entity_type__c String?
    cin__c String?
    pan__c String?
    website__c String?
    license_title__c String?
    license_number__c String?
    account_id Int?
    creaed_date__c DateTime? @default(now())
}

model vendor__c {
    id Int @id @default(autoincrement())
    name__c String?
    phone__c String?
    email__c String? @unique
    gstin__c String?
    address__c String?
    entity_type__c String?
    license_title__c String?
    license_number__c String?
    account_id Int?
    created_date__c DateTime? @default(now())
}

model product__c {
    id Int @id @default(autoincrement())
    name__c String?
    category__c String?
    sku__c String?
    barcode__c String?
    hsn_code__c String?
    tax__c Float?
    unit__c Float?
    mrp__c Float?
    is_sales__c Boolean?
    is_purchase__c Boolean?
    sales_rate__c Float?
    purchase_rate__c Float?
    sales_description__c String?
    purchase_description__c String?
    description__c String?
    image_url__c String?
    mesure_unit__c String?
    account_id Int? @unique()
    created_date__c DateTime? @default(now())
    type__c String?
}

model expense__c {
    id Int @id @default(autoincrement())
    account_id Int? @unique
    vendor_invoice_no__c String?
    nature_of_expense__c String?
    hsn__c String?
    amount__c Float?
    date_of_expense__c DateTime?
    vendor_gst__c Float?
    vendor_name__c String?
    tax_rate__c Float?
    cess__c String?
    igst__c Float?
    total__c Float?
    amount_paid__c String?
    status__c String?
    paid_till__c Float?
    payment_date__c DateTime?
    mode_of_payment__c String?
    created_date__c DateTime? @default(now())
}

model purchase__c {
    id Int @id @default(autoincrement())
    account_id Int? @unique
    purchase_invoice_no__c Int?
    date__c DateTime?
    vendor_gstin__c String?
    delivery_date__c DateTime?
    vendor_name__c String?
    vendor_address__c String?
    vendor_mob_no__c Int?
    delivery_address__c String?
    place_of_supply__c String?
    delivery_to__c String?
    shipment_preference__c String?
    tax_type__c String?
    subtotal__c Float?
    cgst__c Float?
    sgst__c Float?
    shipping_charges__c Float?
    amount_paid__c Float?
    balance_due__c Float?
    total_amount__c Float?
    purchase_note__c String?
    created_date__c DateTime? @default(now())
    status__c String?
}

model purchase_order__c {
    id Int @id @default(autoincrement())
    purchase_id__c Int? @unique
    product_name__c String?
    description__c String?
    hsn__c String?
    rate__c Float?
    quantity__c Int?
    tax_rate__c Float?
    discount__c String?
    amount__c Float?
}

model payment_out__c {
    id Int @id @default(autoincrement())
    account_id Int? @unique
    payment_type__c String?
    payment_date__c DateTime?
    name__c String?
    mobile_no__c String?
    email__c String?
    amount__c Float?
    payment_mode String?
    note__c String?
    created_date__c DateTime? @default(now())
}

model debit_note__c {
    id Int @id @default(autoincrement())
	debit_note_no__c String?
    purchase_invoice_no__c Int?
    date__c DateTime?
    vendor_gstin__c String?
    purchase_date__c DateTime?
