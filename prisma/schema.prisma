}

model pre_purchase_order__c {
    id Int @id @default(autoincrement())
    purchase_order_id__c Int? @unique
    product_name__c String?
    description__c String?
    hsn__c String?
    rate__c Float?
    quantity__c Int?
    tax_rate__c Float?
    discount__c String?
    amount__c Float?
}